<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.myshop.mapper.TestModelMapper">
	
	<select id="selectById" resultType="TestModel" parameterType="TestModel">
		select 
			main.name,
			main.age 
		from test_model main
		where main.id = #{id}
	</select>
	
	
</mapper>